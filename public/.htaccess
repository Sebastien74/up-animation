## HEADER ##
<IfModule mod_headers.c>

	Header unset ETag
    Header set Connection keep-alive
    AddDefaultCharset UTF-8
    AddCharset UTF-8 .htm .html .css .js .woff .woff2

	# Header set Link "</fonts/custom-font.woff2>; rel=preload; as=font; type=font/woff2; crossorigin=anonymous"
	Header append Link "<https://fonts.googleapis.com>; rel=preconnect"
	Header append Link "<https://fonts.gstatic.com>; rel=preconnect; crossorigin"

    # 1 year: 60 seconds Ã— 60 minutes Ã— 24 hours Ã— 365 days = 31536000 seconds
    <FilesMatch "\.(ico|jpe?g|png|gif|svg|webp|css|js|woff2?)$">
        Header set Cache-Control "max-age=31557600, public, immutable"
    </FilesMatch>

    <FilesMatch "\.(mp4|webm|ogv|ogg|mov|avi|mpeg)$">
		Header set Cache-Control "public, max-age=31536000, immutable"
        Header unset Pragma
        Header unset ETag
    </FilesMatch>

    <FilesMatch ".(eot|otf|svg|ttf|woff2?)$">
        Header set Access-Control-Allow-Origin "*"
    </FilesMatch>

    <FilesMatch "\.js$">
        Header set Cache-Control "max-age=31536000, public, immutable"
    </FilesMatch>

	<FilesMatch "\\.(x?html?|php)$">
	    Header set Cache-Control "no-cache, no-store, must-revalidate"
	</FilesMatch>

	<FilesMatch "\\.(html|htm|json)$">
        Header set Cache-Control "max-age=7200, public"
	</FilesMatch>

    # To deindex files
	<FilesMatch "\\.(doc|docx|pdf)$">
	    Header set X-Robots-Tag "noindex, nofollow"
	</FilesMatch>

	FileETag None

    Header unset Pragma
    Header unset ETag

    #Header always edit Set-Cookie ^(.*)$ $1;HttpOnly;Secure
    #Header set X-XSS-Protection "1; mode=block"
#    Header always set X-FRAME-OPTIONS "DENY"
	#Header always append X-Frame-Options SAMEORIGIN
#	Header set X-Content-Type-Options nosniff

</IfModule>

## EXPIRES CACHING ##
<IfModule mod_expires.c>

    ExpiresActive On
	ExpiresDefault "access plus 2 months"

	# Set up caching on media files for 1 month
	<FilesMatch "\.(flv|ico|swf|jgz|ttf|otf|woff|woff2|css|js|mp3|pdf|webmanifest)$">
	    ExpiresDefault "access plus 1 year"
	</FilesMatch>

    <filesMatch ".(css|jpg|jpeg|png|webp|gif|js|ico)$">
        Header set Cache-Control "max-age=31536000, public"
    </filesMatch>

	# Set up caching on images for 1 week
	<FilesMatch "\.(gif|jpe?g|png|svg|webp)$">
	    ExpiresDefault "access plus 1 year"
	</FilesMatch>

	# Set up 1 hour caching on commonly updated files
	<FilesMatch "\.(xml|xsl|html|htm|txt|json)$">
	    ExpiresDefault "access plus 1 week"
	</FilesMatch>

	# Force no caching for dynamic files
	<FilesMatch "\.(pl|php|cgi|spl|scgi|fcgi)$">
	    ExpiresActive Off
	</FilesMatch>

    AddType application/vnd.ms-fontobject .eot
    AddType application/x-font-ttf .ttf
    AddType application/x-font-opentype .otf
    AddType application/x-font-woff .woff
    AddType application/font-woff2 woff2
