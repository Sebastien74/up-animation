AddDefaultCharset UTF-8

# Protecting sensitive files start
<FilesMatch "(\.env|\.git|\.htaccess|composer\.(json|lock)|backup\.sql|config\.php|\.twig|\.yml)$">
    Require all denied
</FilesMatch>
<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_URI} ^/BitKeeper
    RewriteRule ^ - [F,L]
</IfModule>
# Protecting sensitive files end

<IfModule mod_rewrite.c>

    Options -MultiViews
	Options -Indexes

    # Replace Options -MultiViews for 1&1
#    Options +FollowSymLinks
#    RewriteBase /

    RewriteEngine On

	# ðŸ”’ Redirect sensitive paths to denied.php (permanent redirect)
	RewriteCond %{REQUEST_URI} (^|/)(wp-|cgi-bin|xwiki|player|casino|poker|slot|adm/js|adm/plugins|themes/seotheme|doc/function\.php|as/function|function\.php|config\.env|post\.php|smartoptimizer|seotheme|twentytwenty|autoload_classmap/post\.php)(/|$) [NC,OR]
	RewriteCond %{REQUEST_URI} (^|/)(credentials\.json|app\.config\.json)$ [NC]
	RewriteRule .* /denied.php [L,R=301]

	# ðŸ”’ Block any suspicious query string ending with sensitive file extensions
	RewriteCond %{QUERY_STRING} \.(html|well-known|bak|py|svn|rb|env) [NC]
	RewriteRule ^ /denied.php? [L,R=301]

	# ðŸ”’ Block direct access to sensitive file types
	<FilesMatch "\.(html|well-known|bak|py|svn|rb|env|sh|log|sql)$">
	    RewriteRule ^ /denied.php [L,R=301]
	</FilesMatch>

    RewriteCond %{REQUEST_URI} \.(jpe?g|svg|gif|png|pdf|docx|xlsx|css|js)$ [NC]
    RewriteRule ^build/back/ security.php [QSA,L]

    RewriteCond %{REQUEST_URI} \.(jpe?g|gif|png|heic|pdf|docx|xlsx|css|js)$ [NC]
    RewriteRule ^uploads/emails/ security.php [QSA,L]

    RewriteCond %{REQUEST_URI} \.(json)$ [NC]
    RewriteRule ^js/fosjsrouting/ fosjsrouting.php [QSA,L]

    # Check to see if browser can accept gzip files. If so and we have it - serve it!
    ReWriteCond %{HTTP:accept-encoding} gzip
    RewriteCond %{HTTP_USER_AGENT} !Safari

    # Make sure there's no trailing .gz on the url
    ReWriteCond %{REQUEST_FILENAME} !^.+\.gz$

    # Check to see if a .gz version of the file exists.
    RewriteCond %{REQUEST_FILENAME}.gz -f

    # All conditions met so add .gz to URL filename (invisibly)
    RewriteRule ^(.+) $1.gz [QSA,L]

    RewriteCond %{REQUEST_URI}::$1 ^(/.+)/(.*)::\2$
    RewriteRule ^(.*) - [E=BASE:%1]

    RewriteCond %{HTTP:Authorization} .+
    RewriteRule ^ - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]

    RewriteCond %{ENV:REDIRECT_STATUS} ^$
    RewriteRule ^index\.php(?:/(.*)|$) %{ENV:BASE}/$1 [R=301,L]

    RewriteCond %{REQUEST_FILENAME} -f
    RewriteRule ^ - [L]

    RewriteRule ^ %{ENV:BASE}/index.php [L]

</IfModule>

<IfModule !mod_rewrite.c>
    <IfModule mod_alias.c>
        RedirectMatch 302 ^/$ /index.php/
    </IfModule>
</IfModule>

